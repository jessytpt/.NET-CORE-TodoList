<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="SettingsNET.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>SettingsNET.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="SettingsNET.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>SettingsNET.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
